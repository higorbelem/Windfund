{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",             
    "module": "commonjs",            
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,  
    "strict": true,  
    "skipLibCheck": true,
    "baseUrl": "./",
    "resolveJsonModule": true,
    "paths": {
      "#/*": ["src/*"]
    }            
  },
  "extends": "@tsconfig/react-native/tsconfig.json",
  "include": ["src", "src/components", "src/@types/custom.d.ts"]
}
